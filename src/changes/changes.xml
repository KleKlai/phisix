<document xmlns="http://maven.apache.org/changes/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
    <properties>
        <title>phisix</title>
    </properties>
    <body>
        <release version="1.6.1" date="2019-09-26" description="bump dependencies">
            <action dev="edgar.dalmacio@gmail.com" type="update">
                bump dependencies
            </action>
        </release>

        <release version="1.6" date="2017-03-02" description="added 'Referer' HTTP request header">
            <action dev="edgar.dalmacio@gmail.com" type="fix">
                added 'Referer' HTTP request header
            </action>
        </release>

        <release version="1.5" date="2016-12-22" description="drop decimal places in totalVolume to allow stock indexes">
            <action dev="edgar.dalmacio@gmail.com" type="fix">
                drop decimal places in totalVolume to allow stock indexes
            </action>
            <action dev="edgar.dalmacio@gmail.com" type="delete">
                cleanup
            </action>
        </release>

        <release version="1.4" date="2016-03-27" description="Added cron.xml">
            <action dev="edgar.dalmacio@gmail.com" type="add">
                Not getting any result for a given stock/date request (https://github.com/edgedalmacio/phisix/issues/15)
            </action>
        </release>

        <release version="1.3" date="2014-09-13" description="Added JSONP Support">
            <action dev="edgar.dalmacio@gmail.com" type="add">
                Added JSONP Support (https://github.com/edgedalmacio/phisix/issues/6)
            </action>
        </release>

        <release version="1.2" date="2014-05-24" description="Load Balancing">
            <action dev="edgar.dalmacio@gmail.com" type="add">
                Added load balancing to http://phisix-api2.appspot.com
            </action>
        </release>

        <release version="1.1" date="2014-01-31" description="Cache stocks for one (1) minute">
            <action dev="edgar.dalmacio@gmail.com" type="add">
                Cache stocks for one (1) minute
            </action>
        </release>

        <release version="1.0" date="2014-01-27" description="Allow GET /stocks/{symbol}.{yyyy-MM-dd}.{xml|json}">
            <action dev="edgar.dalmacio@gmail.com" type="add">
                Allow GET /stocks/{symbol}.{yyyy-MM-dd}.{xml|json}
            </action>
        </release>

        <release version="0.9" date="2013-09-05" description="added /stocks/cache to store the days data for later retrieval">
            <action dev="edgar.dalmacio@gmail.com" type="add">
                added /stocks/archive to store the days data for later retrieval
            </action>
        </release>

        <release version="0.8" date="2013-08-06" description="getting stocks by symbol is no longer case-sensitive">
            <action dev="edgar.dalmacio@gmail.com" type="fix">
                getting stocks by symbol is no longer case-sensitive
            </action>
        </release>

        <release version="0.7" date="2013-08-01" description="Allow GET /stocks/{symbol}.{xml|json}">
            <action dev="edgar.dalmacio@gmail.com" type="add">
                Allow GET /stocks/{symbol}.{xml|json}
            </action>
        </release>

        <release version="0.6" date="2013-07-24" description="added &quot;Last-Modified&quot; response header">
            <action dev="edgar.dalmacio@gmail.com" type="add">
                added "Last-Modified" response header
            </action>
        </release>

        <release version="0.5" date="2013-06-26" description="using mvn site output as content for main page">
            <action dev="edgar.dalmacio@gmail.com" type="update">
                using mvn site output as content for main page
            </action>
        </release>

        <release version="0.4" date="2013-06-08" description="fixed &quot;as of&quot; date to ASIA/MANILA Time Zone">
            <action dev="edgar.dalmacio@gmail.com" type="fix">
                fixed "as of" date to ASIA/MANILA Time Zone
            </action>
        </release>

        <release version="0.3" date="2013-01-24" description="respond with HTTP 504 (Gateway Timeout) on SocketTimeoutException">
            <action dev="edgar.dalmacio@gmail.com" type="fix">
                respond with HTTP 504 (Gateway Timeout) on SocketTimeoutException
            </action>
        </release>

        <release version="0.2" date="2012-11-25" description="updated source feed">
            <action dev="edgar.dalmacio@gmail.com" type="update">
                updated source feed
            </action>
            <action dev="edgar.dalmacio@gmail.com" type="fix">
                set response content type to text/xml
            </action>
            <action dev="edgar.dalmacio@gmail.com" type="add">
                added json endpoint
            </action>
        </release>

        <release version="0.1" date="2012-02-08" description="Initial Release">
            <action dev="edgar.dalmacio@gmail.com" type="add">
                added handling of GET /stocks.xml
            </action>
        </release>

    </body>
</document>
